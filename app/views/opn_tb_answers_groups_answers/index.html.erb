        <td><%= link_to 'Edit', edit_opn_tb_answers_groups_answer_path(opn_tb_answers_groups_answer) %></td>
        <td><%= link_to 'Destroy', opn_tb_answers_groups_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
